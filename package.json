{
  "name": "generative",
  "version": "0.1.0",
  "private": true,
  "license": "Creative Commons",
  "scripts": {
    "predev": "npm run prepare",
    "dev": "craco start",
    "prebuild": "npm run prepare",
    "build": "cross-env GENERATE_SOURCEMAP=false craco build",
    "prepare": "node ./scripts/prepareSketchLists.js",
    "test": "craco test",
    "lint": "eslint src/**/*.js",
    "stylelint": "stylelint src/**/*.scss",
    "lint:fix": "npm run lint -- --fix",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "prettier --write \"src/**/*.{js,json,css,scss,md}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "public/static/assets/preview/*.png": [
      "node scripts/preparePreview.js"
    ],
    "src/**/*.{js,ts,tsx,css,scss}": [
      "prettier --write",
      "suppress-exit-code eslint --fix",
      "git add"
    ],
    "*.{json,js,ts,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "homepage": ".",
  "dependencies": {
    "@babylonjs/core": "5.0.0-alpha.12",
    "@babylonjs/inspector": "5.0.0-alpha.12",
    "@babylonjs/loaders": "5.0.0-alpha.12",
    "@babylonjs/materials": "5.0.0-alpha.12",
    "@craco/craco": "^6.1.0",
    "animejs": "^3.2.1",
    "bunny": "1.0.1",
    "canvas-sketch": "0.7.4",
    "canvas-sketch-util": "1.10.0",
    "chaikin-smooth": "1.0.4",
    "clsx": "1.1.1",
    "compression-webpack-plugin": "6.1.1",
    "craco-esbuild": "^0.2.0",
    "cross-env": "^7.0.3",
    "customize-cra": "1.0.0",
    "d3-array": "2.11.0",
    "dat.gui": "^0.7.7",
    "gl-matrix": "3.3.0",
    "glsl-noise": "0.0.0",
    "glslify": "7.1.1",
    "glslify-loader": "2.0.0",
    "gsap": "^3.6.0",
    "i18next": "19.8.5",
    "immer": "8.0.1",
    "lodash": "4.17.20",
    "nice-color-palettes": "^3.0.0",
    "node-sass": "4.13.1",
    "point-in-polygon": "1.0.1",
    "popmotion": "^9.1.0",
    "preval.macro": "5.0.0",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.2",
    "ray-sphere-intersection": "^1.0.0",
    "react": "17.0.1",
    "react-app-rewire-hot-loader": "2.0.1",
    "react-app-rewired": "2.1.8",
    "react-dom": "17.0.1",
    "react-hot-loader": "4.13.0",
    "react-i18next": "11.8.5",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "soundcloud-audio": "1.4.0",
    "stats.js": "0.17.0",
    "three113": "npm:three@0.113.2",
    "three125": "npm:three@0.125.2",
    "web-audio-analyser": "2.0.1"
  },
  "devDependencies": {
    "@jimp/plugin-resize": "0.16.1",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "jimp": "0.16.1",
    "lint-staged": "10.5.3",
    "localhost-certs": "^1.0.0",
    "prettier": "2.2.1",
    "shelljs": "0.8.4",
    "stylelint": "13.9.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "suppress-exit-code": "^1.0.0"
  }
}
